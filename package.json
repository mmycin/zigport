{
  "name": "zigport",
  "version": "1.2.2",
  "description": "A CLI tool that automatically generates TypeScript FFI bindings for Zig libraries, making it easy to call Zig functions from JavaScript/TypeScript in Bun projects",
  "main": "dist/index.js",
  "bin": {
    "zigport": "./bin/zigport.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "zig",
    "typescript",
    "ffi",
    "bun",
    "bindings"
  ],
  "author": "Tahcin Ul Karim (Mycin)",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "chalk": "^5.3.0",
    "glob": "^10.3.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.4.5",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmycin/zigport.git"
  },
  "bugs": {
    "url": "https://github.com/mmycin/zigport/issues"
  },
  "homepage": "https://github.com/mmycin/zigport#readme"
}